<Page x:Class="WpfAppVmeste.View.PageViborPersonaja"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfAppVmeste.View"
      mc:Ignorable="d" 
      d:DesignHeight="1000" d:DesignWidth="1000"
      Title="PageViborPersonaja">
    <Grid Background="White">
        <Grid>
            <Grid Grid.Column="1">
                <Grid.RowDefinitions>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition Height="125"/>
                    <RowDefinition/>
                </Grid.RowDefinitions>
                <Image Source="/images/Свофорд.jpg" Grid.Row="0" Stretch="Fill" MaxWidth="300"/>
                <Image Source="/images/Папич серьезны.jpg" Grid.Row="1" Stretch="Fill" MaxWidth="300"/>
                <Image Source="/images/пудж.jpg" Grid.Row="2" Stretch="Fill" MaxWidth="300"/>
                <Image Source="/images/Даун.jpg" Grid.Row="3" Stretch="Fill" MaxWidth="300"/>
                <Image Source="/images/чел_чипсы_хавает.jpg" Grid.Row="4" Stretch="Fill" MaxWidth="300"/>
                <Image Source="/images/Хайзенберг.jpg" Grid.Row="5" Stretch="Fill" MaxWidth="300"/>
                <Image Source="/images/Гламурный мейк))).jpg" Grid.Row="6" Stretch="Fill" MaxWidth="300"/>
                <Image Source="/images/бубузьяна.jpg" Grid.Row="7" Stretch="Fill" MaxWidth="300"/>
            </Grid>
            <Grid Grid.Column="0">
                <Grid.RowDefinitions>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                </Grid.RowDefinitions>
                <CheckBox Content="" HorizontalAlignment="Center" Grid.Row="0"  VerticalAlignment="Center" IsChecked="{Binding IsCheckedCheckBox1}"/>
                <CheckBox Content="" HorizontalAlignment="Center" Grid.Row="1"  VerticalAlignment="Center" IsChecked="{Binding IsCheckedCheckBox2}"/>
                <CheckBox Content="" HorizontalAlignment="Center" Grid.Row="2"  VerticalAlignment="Center" IsChecked="{Binding IsCheckedCheckBox3}"/>
                <CheckBox Content="" HorizontalAlignment="Center" Grid.Row="3"  VerticalAlignment="Center" IsChecked="{Binding IsCheckedCheckBox4}"/>
                <CheckBox Content="" HorizontalAlignment="Center" Grid.Row="4"  VerticalAlignment="Center" IsChecked="{Binding IsCheckedCheckBox5}"/>
                <CheckBox Content="" HorizontalAlignment="Center" Grid.Row="5"  VerticalAlignment="Center" IsChecked="{Binding IsCheckedCheckBox6}"/>
                <CheckBox Content="" HorizontalAlignment="Center" Grid.Row="6"  VerticalAlignment="Center" IsChecked="{Binding IsCheckedCheckBox7}"/>
                <CheckBox Content="" HorizontalAlignment="Center" Grid.Row="7"  VerticalAlignment="Center" IsChecked="{Binding IsCheckedCheckBox8}"/>
            </Grid>
            <Grid Grid.Column="2">
                <Grid.RowDefinitions>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                </Grid.RowDefinitions>
                <Label Grid.Row="0" Content="СЭР ДА СЭР" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="25"/>
                <Label Grid.Row="1" Content="14.1 наилучший возвраст" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="25"/>
                <Label Grid.Row="2" Content="Фарш не провернуть назад, и мясо из котлет не восстановишь" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="25"/>
                <Label Grid.Row="3" Content="ЫЫЫЫЫЫЫЫЫЫЫ" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="25"/>
                <Label Grid.Row="4" Content="Сожру с пивом" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="25"/>
                <Label Grid.Row="5" Content="Say may name (Haizenberg)" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="25"/>
                <Label Grid.Row="6" Content="Нищита домой" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="25"/>
                <Label Grid.Row="7" Content="угагагагага" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="25"/>
            </Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="10*"/>
                <ColumnDefinition Width="37*"/>
                <ColumnDefinition Width="53*"/>
            </Grid.ColumnDefinitions>
        </Grid>
    </Grid>
</Page>
